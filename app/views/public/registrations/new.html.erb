<div class="container">
    <div class="ml-4">
ã€€  <h2 class="text-left font-weight-bold"><i class="fas fa-fish"></i>Sign up<i class="fas fa-fish"></i></h2>

    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <%= render "public/shared/error_messages", resource: resource %>

  <div class="field">
    <i class="fas fa-fish"></i><%= f.label :"first_name" %><br />
    <%= f.text_field :fast_name, autofocus: true, autocomplete: "fast_name" %>
  </div>

  <div class="field">
    <i class="fas fa-fish"></i><%= f.label :"last_name" %><br />
    <%= f.text_field :last_name, autofocus: true, autocomplete: "last_name" %>
  </div>

  <div class="field">
    <i class="fas fa-fish"></i><%= f.label :"first_name_kana" %><br />
    <%= f.text_field :fast_name_kana, autofocus: true, autocomplete: "fast_name_kana" %>
  </div>

   <div class="field">
    <i class="fas fa-fish"></i><%= f.label :"last_name_kana" %><br />
    <%= f.text_field :last_name_kana, autofocus: true, autocomplete: "last_name_kana" %>
  </div>

  <div class="field">
    <i class="fas fa-fish"></i><%= f.label :"Email" %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <i class="fas fa-fish"></i><%= f.label :"Post code" %><br />
    <%= f.text_field :postal_code, autofocus: true, autocomplete: "postal_code" %>
  </div>

  <div class="field">
    <i class="fas fa-fish"></i><%= f.label :"Adress" %><br />
    <%= f.text_field :address, autofocus: true, autocomplete: "aderess" %>
  </div>

  <div class="field">
    <i class="fas fa-fish"></i><%= f.label :"Phone number(no hyphen)" %><br />
    <%= f.text_field :phone_number, autofocus: true, autocomplete: "phone_number" %>
  </div>

  <div class="field">
    <i class="fas fa-fish"></i><%= f.label :"Password" %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "new-password" %>
  </div>

  <div class="field">
    <i class="fas fa-fish"></i><%= f.label :"Password(for confirmation)" %><br />
    <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
  </div>

  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>

<%= render "public/shared/links" %>
 </div>
    </div>
    </div>
